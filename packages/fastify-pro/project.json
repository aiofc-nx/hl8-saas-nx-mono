{
  "name": "fastify-pro",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/fastify-pro/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/fastify-pro",
        "main": "packages/fastify-pro/src/index.ts",
        "tsConfig": "packages/fastify-pro/tsconfig.lib.json",
        "assets": [
          "packages/fastify-pro/*.md"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/fastify-pro/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/fastify-pro/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  }
}